# Skip optional dependencies to reduce installation time
optional=false

# Use npm cache to speed up installations
cache=.npm-cache

# Set timeout to 30 minutes
timeout=1800000

# Skip Chromium download for Puppeteer
puppeteer_skip_chromium_download=true

# Use production install
production=true

# Disable audit during install
audit=false

# Disable fund messages
fund=false 